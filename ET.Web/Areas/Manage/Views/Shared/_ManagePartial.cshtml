<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@RenderSection("Title", required: false)</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    @Styles.Render("~/Content/baseStyle", "~/Content/Manage/manageStyle")
    <link href="@Url.Content("~/Res/easyui/themes/default/easyui.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Res/easyui/themes/icon.css")" rel="stylesheet" type="text/css" />
    @RenderSection("Styles", required: false)
    @if (PublicHelper.IsInternetExplorer())
    {
        <script src="~/Scripts/JQuery/jquery-1.11.1.min.js" type="text/javascript"></script>
    }
    else
    {
        <script src="~/Scripts/JQuery/jquery-2.1.1.min.js" type="text/javascript"></script>
    }
    <script src="~/Scripts/JQuery/jquery.form.js" type="text/javascript"></script>
    <script src="~/Scripts/JQuery/jquery.parser.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/common/common.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Res/easyui/jquery-easyui-min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Res/easyui/easyui-extend.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Res/easyui/locale/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("/Scripts/common/jquery.topbar.js")"></script>

    @RenderSection("Header", required: false)
</head>
<body>
    @RenderBody()
    @RenderSection("BodyExtend", required: false)
</body>
</html>
