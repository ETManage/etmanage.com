@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.PageTitle</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link rel="icon" href="/favicon.ico">
    @Scripts.Render("~/Scripts/jquery")
    <link href="@Url.Content("~/Res/easyui/themes/default/easyui.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Res/easyui/themes/icon.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Res/easyui/jquery-easyui-min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Res/easyui/easyui-extend.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Res/easyui/locale/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <link href="@Url.Content("~/Content/Manage/Default.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/Manage/Default.js")" type="text/javascript"></script>
</head>
<body class="easyui-layout" scroll="no" id="layoutMain">
    <div id="mainBody">
        <div id="cloud1" class="cloud">
        </div>
        <div id="cloud2" class="cloud">
        </div>
    </div>
    <noscript>
        <div class="JsTip">
            <img src="/images/Manage/noscript.gif" alt='抱歉，请开启脚本支持！' />
        </div>
    </noscript>
    <div region="north" split="true" class="PageTop" border="false">
        <div class="logintop">
            <span>
                <img alt="" src="/Images/Manage/logo.png" width="30" height="30" align="absmiddle" />企业信息化平台
            </span>
            <ul>
                <li>欢迎&nbsp;<b>@ViewBag.STU_CNNAME</b></li>
                <li>当前在线：@ViewBag.ONLINE_USER_COUNT</li>
                <li><a href="/">转到首页</a></li>
                <li><a href="#" id="loginOut">安全退出</a></li>
            </ul>
        </div>
    </div>
    <div region="south" split="false" class="PageFooter" border="false">
        <div class="loginbm">
            @ViewBag.CompanyName 版权所有 @ViewBag.PageVer
        </div>
    </div>
    <div region="west" hide="true" split="true" title="导航菜单" class="PageMainLeft">
        <div id="nav" class="easyui-accordion" fit="true" border="false">
            <!--  导航内容 -->
        </div>
    </div>
    <div id="mainPanle" region="center" class="PageMainCenter">
        <div id="tabs" class="easyui-tabs" border="false">
            <div title="管理首页" class="tabsMain">
                欢迎使用<br />

            </div>
        </div>
    </div>
    @*   <div region="east" title="快捷方式" split="true" class="PageMainRight">
            <div class="easyui-calendar" border="false">
            </div>
        </div>*@
    <!--选项卡右键菜单-->
    <div id="TabMenuStrip" class="easyui-menu">
        <div id="menu-tabupdate">
            刷新
        </div>
        <div class="menu-sep">
        </div>
        <div id="menu-tabclose">
            关闭
        </div>
        <div id="menu-tabcloseall">
            全部关闭
        </div>
        <div id="menu-tabcloseother">
            除此之外全部关闭
        </div>
        <div class="menu-sep">
        </div>
        <div id="menu-tabcloseright">
            当前页右侧全部关闭
        </div>
        <div id="menu-tabcloseleft">
            当前页左侧全部关闭
        </div>
        <div class="menu-sep">
        </div>
        <div id="mm-exit">
            退出
        </div>
    </div>
    <script type="text/javascript">
        $("#tabs").css("height", $(document).height() - 110);
        //加载模块
        startFloatBG();
    </script>

</body>
</html>
