    <!--2/如果stateConnectionString的值不是127.0.0.1或者localhost等代表本地地址的值，需要修改注册表：HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Services\aspnet_state \Parameters 节点 → 将 AllowRemoteConnection 的键值设置成“1”（1 为允许远程电脑的连接，0 代表禁止）→ 设置 Port (端口号) -->
    <!--<sessionState mode="InProc" timeout="60"/>-->
    <globalization requestEncoding="utf-8" responseEncoding="utf-8" fileEncoding="utf-8" culture="zh-CN"/>
    <authentication mode="Windows" lockItem="true"/>
    <!-- RemoteOnly在客户端隐藏错误，Off在客户端直接显示错误-->
    <customErrors mode="RemoteOnly" defaultRedirect="/pageerror/error404">
      <error statusCode="403" redirect="/pageerror/error404"/>
      <error statusCode="404" redirect="/pageerror/error404"/>
    </customErrors>
